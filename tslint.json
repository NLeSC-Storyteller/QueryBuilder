{
    "linterOptions": {
        "typeCheck": true
    },
    "extends": [
        "@nlesc/tslint-config-react-app"
    ],
    "rules": {
        "no-for-in-array": false,
        "restrict-plus-operands": false,
        "no-console": [false],
        "indent": [true, 4],
        "react-a11y-event-has-role": false,
        "no-http-string": [true, "http://localhost/?."]
    }
}
