{
  "name": "querybuilder ",
  "version": "0.0.0",
  "description": "QueryBuilder",
  "main": "./dist/querybuilder.min.js",
  "scripts": {
    "clean-build": "trash build/",
    "clean-cover": "trash docs/sites/coverage/",
    "clean-dist": "trash dist/",
    "clean-downloaded": "trash node_modules/",
    "clean": "npm run clean-build & npm run clean-dist & npm run clean-sites",
    "clean-sites": "npm run clean-cover & npm run clean-tsdoc",
    "clean-tsdoc": "trash docs/sites/tsdoc/",
    "cover": "npm run test && npm run remap-istanbul-json && npm run remap-istanbul-html",
    "csslint": "cat src/components/*.css > dist/querybuilder.css && csslint --config=.csslintrc --format=compact dist/querybuilder.css",
    "dist": "browserify src/index.tsx --debug --standalone querybuilder -p [ tsify --project . ] > build/querybuilder-with-source-maps.js",
    "everything": "npm run tsdoc && npm run cover",
    "exorcist": "<build/querybuilder-with-source-maps.js exorcist dist/querybuilder.js.map > dist/querybuilder.js",
    "jshint": "jshint test/*.js",
    "jslint": "jslint test/*.js",
    "lint": "npm run tslint && npm run jshint && npm run jslint && npm run csslint",
    "postdist": "npm run exorcist && echo \"npm run uglifyjs\" does not work in combination with es6 && npm run uglifycss",
    "postversion": "git log --pretty=oneline --max-count 10",
    "precover": "trash docs/sites/coverage/ && npm run jslint && npm run jshint && npm run dist",
    "predist": "trash build/ && mkdirp build && trash dist/ && mkdirp dist && npm run tslint && npm run csslint",
    "pretsdoc": "npm run clean-tsdoc && mkdirp docs/sites/tsdoc",
    "preversion": "npm run purge && npm install && typings install && npm run everything",
    "purge": "npm run clean & npm run clean-downloaded",
    "remap-istanbul-html": "remap-istanbul --input docs/sites/coverage/coverage-remapped.json --output docs/sites/coverage/remapped --type html --exclude node_modules",
    "remap-istanbul-json": "remap-istanbul --input docs/sites/coverage/coverage.json --output docs/sites/coverage/coverage-remapped.json",
    "test": "karma start karma.conf.js",
    "tsdoc": "typedoc --out docs/sites/tsdoc/ .",
    "tslint": "tslint --config tslint.json ./src/**/*.ts",
    "uglifycss": "uglifycss dist/querybuilder.css > dist/querybuilder.min.css",
    "uglifyjs": "echo uglifyjs dist/querybuilder.js --output dist/querybuilder.min.js --source-map dist/querybuilder.min.js.map --screw-ie8 --in-source-map dist/querybuilder.js.map --source-map-url querybuilder.min.js.map"
  },
  "keywords": [
    "typescript",
    "visualization"
  ],
  "author": "Maarten van Meersbergen, Jurriaan H. Spaaks",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/nlesc-storyteller/QueryBuilder.git"
  },
  "devDependencies": {
    "@types/react": "^0.14.44",
    "@types/react-dom": "^0.14.18",
    "browserify": "^13.1.0",
    "browserify-shim": "^3.8.12",
    "csslint": "^1.0.2",
    "exorcist": "^0.4.0",
    "istanbul": "^0.4.4",
    "jasmine-core": "^2.4.1",
    "jshint": "^2.9.3",
    "jslint": "^0.10.3",
    "karma": "^1.2.0",
    "karma-coverage": "^1.1.1",
    "karma-fixture": "^0.2.6",
    "karma-html2js-preprocessor": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-json-fixtures-preprocessor": "0.0.6",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-spec-reporter": "0.0.26",
    "mkdirp": "^0.5.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "remap-istanbul": "^0.6.4",
    "trash-cli": "^1.3.0",
    "tsify": "^1.0.3",
    "tslint": "^3.9.0",
    "typedoc": "^0.4.1",
    "typescript": "^2.0.3",
    "uglify-js": "^2.7.3",
    "uglifycss": "0.0.21"
  },
  "dependencies": {},
  "browser": {},
  "browserify-shim": {},
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "files": [
    "CHANGELOG",
    "dist/querybuilder.js",
    "dist/querybuilder.js.map",
    "dist/querybuilder.min.js",
    "dist/querybuilder.min.js.map",
    "dist/querybuilder.css",
    "dist/querybuilder.min.css",
    "LICENSE",
    "package.json",
    "README"
  ]
}
